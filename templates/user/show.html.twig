{% extends 'baseuser.html.twig' %}
 {% block body %}

<br>
<br>
<br>
<br>

    <table class="table" borde=2>
        <tbody>
            <tr>
                
                <td><img src="{{ asset('images/users/' ~ user.image) }}"  width="150" height="150" class="img-fluid" alt=""></td>
                 
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ user.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ user.prenom }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ user.mail }}</td>
                
            </tr>
            
            <tr>
                <th>Numero_telephone</th>
                <td>{{ user.numeroTelephone }}</td>
            </tr>
            
            <tr>
                <th>Addresse</th>
                <td>{{ user.addresse }}</td>
            </tr>
            
        </tbody>

    </table>
    

{% endblock %}
